{
  "name": "imagekit-react-widgets",
  "private": false,
  "version": "0.1.7",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build:lib": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc -p tsconfig.build.json --noEmit",
    "release": "npm publish --access public"
  },
  "dependencies": {
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "react-router-dom": "^7.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "README.zh-CN.md"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "dist/index.css"
  ],
  "keywords": [
    "imagekit",
    "react",
    "widgets",
    "resource-center",
    "images",
    "video"
  ],
  "homepage": "https://github.com/webTopkee/imagekit-react-widgets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webTopkee/imagekit-react-widgets.git"
  },
  "author": "weber",
  "license": "MIT"
}
